<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="style_p.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    <title>Profile</title>
</head>
<body>

   <div class="outer">
    <div class="dashboard">
        <span class="material-symbols-outlined">
            arrow_back_ios
            </span>
    </div>
    <div class="profile-container">
        <div id="heading">
        <h2>Create Your Profile</h2>
        </div>

        <div class="profile-photo">
            <!-- <label for="profile-image">Profile Image:</label> -->
            <div  for="profile-image" id="preview-image">
                <img src="profile12.png" >
            </div>
            <div id="imp" style="position: relative; align-self: center;">
            <input type="file"  accept="image/*" onchange="previewImage()" >
            </div>
        </div>

        <div class="profile-info">
            <div class="input-group mb-3">
                <span class="input-group-text">Name</span>
                <input type="text" class="form-control">
            </div>
            <div class="input-group mb-3">
                <span class="input-group-text">School Name</span>
                <input type="text" class="form-control">
            </div>
            <div class="input-group mb-3">
                <span class="input-group-text">Class</span>
                <input type="text" class="form-control">
            </div>
            <div class="input-group mb-3">
                <span class="input-group-text">About Me</span>
                <input type="text" class="form-control">
            </div>
        </div>
        <button class="btn1" onclick="createProfile()">Create Profile</button>
    </div>
</div>

<script src="app2.js"></script>
<script src="https://code.jquery.com/jquery-3.6.4.min.js" integrity="sha384-oG5F8R3P3U1wD6z1aPCCQqF9SA5wt5qzynx9M2p4on7a8QQkgsiJCAksTUpwN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
